<?xml version="1.0"?>
<project>
  <properties>
    <tile-apicheck.contract>1.1</tile-apicheck.contract>
  </properties>

  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>clirr-maven-plugin</artifactId>
        <version>2.8</version>
        <executions>
          <execution>
            <id>api-check</id>
            <phase>test</phase>
            <goals>
              <goal>check</goal>
            </goals>
            <configuration>
              <comparisonVersion>[${tile-apicheck.contract}]</comparisonVersion>
            </configuration>
          </execution>
        </executions>
      </plugin>

    </plugins>
  </build>
</project>
